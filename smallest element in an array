//smallest element in an array
#include<stdio.h>
int main()
{
  int a[20],i,n,smallest;
  printf("Enter size of elements:");
  scanf("%d",&n);
  printf("Enter array elements");
 
  for(i=0;i<n;i++)
  {
    scanf("%d",&a[i]);
  }
   smallest=a[0];
  for(i=1;i<n;i++)
 {
   if(smallest>a[i])
smallest=a[i];
 }
printf("smallest element in an array is %d",smallest);
  return 0;
}
