//triangle is eqililateral,scalene,isosceles
#include<stdio.h>
void main()
{
  int a,b,c;
  printf("ENTER ANY 3 SIDES OF A TRIANGLE");
  scanf("%d %d %d",&a,&b,&c);
  if(a==b&&b==c)
  {
    printf("EQUILATERAL TRIANGLE");
  }
  else if(a==b||b==c||a==c)
  printf("ISOSCELES TRIANGLE");
  else
  {
    printf("SCALENE TRIANGLE");
  }
}
