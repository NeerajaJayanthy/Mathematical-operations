// size of float char and double
#include<stdio.h>
int main()
{
  int a;
  float b;
  char c;
  double d;
  printf("size of int=%ld bytes\n",sizeof(a));
  printf("size of float=%ld bytes\n",sizeof(b));
  printf("size of char=%ld bytes\n",sizeof(c));
  printf("size of double=%ld bytes\n",sizeof(d));
  return 0;
}
