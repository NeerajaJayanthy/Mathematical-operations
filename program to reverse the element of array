//program to reverse the element of array
#include <stdio.h>
int main()
{
  int a[20],n,i;
  printf("Enter the size of the array:");
  scanf("%d",&n);
  printf("Enter the array elements:\n");
  for (i=0;i<n;i++) 
  {     
  scanf("%d",&a[i]);   
  }         
  printf("\nArray in reverse order: \n");   
   for (i=n-1;i>= 0;i--) 
  {     
  printf("%d",a[i]);   
  }     
    return 0;    
}   
