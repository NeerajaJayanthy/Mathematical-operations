//find largest element in an array
#include<stdio.h>
int main()
{
  int a[20],i,n,largest;
  printf("Enter size of elements:");
  scanf("%d",&n);
  printf("Enter array elements");
 
  for(i=0;i<n;i++)
  {
    scanf("%d",&a[i]);
  }
   largest=a[0];
  for(i=1;i<n;i++)
 {
   if(largest<a[i])
largest=a[i];
 }
printf("largest element in an array is %d",largest);
  return 0;
}
